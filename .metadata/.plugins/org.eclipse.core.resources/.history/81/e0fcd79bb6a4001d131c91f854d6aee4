
public class Driver {
	public static void main(String[] args) {	    
		Perishable milk = new Perishable(1, "Milk", 2.99, 10, "1 gallon of whole milk", 7);
		Electronic laptop = new Electronic(2, "Laptop", 999.99, 5, "Dell Inspiron 15", "Dell");
		    
		System.out.println("Perishable Product: ");
		System.out.println(milk);
		    
		System.out.println("\nElectronic Product: ");
		System.out.println(laptop);
	}
}
